<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="mainPane" minHeight="400.0" minWidth="600.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
	<top>
	</top>
	<center>
	</center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="fileMenu" mnemonicParsing="false">
            <items>
              <Menu fx:id="exportMenu" mnemonicParsing="false" styleClass="menu-item-first">
            	<items>
              	  <MenuItem fx:id="exportCsvMenuItem" mnemonicParsing="false" styleClass="menu-item-first"/>
                  <MenuItem fx:id="exportPrevMenuItem" mnemonicParsing="false"/>
                </items>
              </Menu>
              <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" />
            </items>
          </Menu>
          <Menu fx:id="radioMenu" mnemonicParsing="false">
            <items>
              <Menu fx:id="serialPortMenu" mnemonicParsing="false" styleClass="menu-item-first">
              	<items></items>
              </Menu>
              <MenuItem fx:id="connectMenuItem" mnemonicParsing="false"/>
              <MenuItem fx:id="disconnectMenuItem" mnemonicParsing="false"/>
            </items>
          </Menu>
            <Menu fx:id="windowMenu" mnemonicParsing="false">
              <items>
                <MenuItem fx:id="preferencesMenuItem" mnemonicParsing="false" styleClass="menu-item-first"/>
              </items>
            </Menu>
          <Menu fx:id="helpMenu" mnemonicParsing="false">
            <items>
              <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" styleClass="menu-item-first"/>
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
