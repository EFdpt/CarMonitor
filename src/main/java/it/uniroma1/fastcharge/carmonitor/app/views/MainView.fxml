<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="mainPane" minHeight="400.0" minWidth="600.0" prefHeight="600.0" prefWidth="800.0" stylesheets="@../assets/stylesheets/application.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
	<top>
	</top>
	<center>
	</center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu fx:id="fileMenu" mnemonicParsing="false">
            <items>
              <Menu mnemonicParsing="false" text="Export" styleClass="menu-item-first">
            	<items>
              	  <MenuItem mnemonicParsing="false" styleClass="menu-item-first" text="Export csv"/>
                  <MenuItem mnemonicParsing="false" text="Export raw"/>
                  <MenuItem mnemonicParsing="false" text="Export previous session"/>
                </items>
              </Menu>
              <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Radio">
            <items>
              <MenuItem mnemonicParsing="false" styleClass="menu-item-first" text="Connect" />
              <MenuItem mnemonicParsing="false" text="Disconnect" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Window">
              <items>
                <MenuItem mnemonicParsing="false" styleClass="menu-item-first" text="Preferences" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" styleClass="menu-item-first" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
